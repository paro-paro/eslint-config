{
  "name": "@paro-paro/eslint-config",
  "type": "module",
  "version": "2.2.0",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "description": "ESLint config preset by @paro-paro",
  "author": "Antonio PG <aparodyg@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/paro-paro/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paro-paro/eslint-config.git"
  },
  "bugs": "https://github.com/paro-paro/eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "printconfig": "eslint --print-config",
    "preview": "eslint-config-inspector",
    "prepare:hooks": "simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-lite": "^0.3.0",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-jsdoc": "^51.0.3",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.2.0",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.2.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@types/node": "^24.0.3",
    "bumpp": "^10.2.0",
    "eslint": "^9.29.0",
    "eslint-typegen": "^2.2.0",
    "lint-staged": "^16.1.2",
    "simple-git-hooks": "^2.13.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
